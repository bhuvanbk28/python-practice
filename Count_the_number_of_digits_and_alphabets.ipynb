      "source": [
        "text = str(input(\"Enter a string: \"))\n",
        "digit=alphabet=0\n",
        "for i in text:\n",
        "  if i.isdigit():\n",
        "    digit+=1\n",
        "  elif i.isalpha():\n",
        "    alphabet+=1\n",
        "print(\"Digit: \",digit)\n",
        "print(\"Alphabet: \",alphabet)"
      ]
